@model MVC5App.Models.Customer
@{
	ViewBag.Title = "Details";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>
<ul>@Model.Name
	@if (@Model.DateOfBirth != null)
	{
		<li>DateOfBirth: @Model.DateOfBirth.Value.ToShortDateString()</li>
	}
	<li>MemberShipType Name: @Model.MemberShipType.Name</li>
	<li>SignUp Fee: @Model.MemberShipType.SingUpFee</li>
	<li>Duration In Month: @Model.MemberShipType.DuratiionInMonth</li>
	<li>Discount Rate: @Model.MemberShipType.DiscountRate%</li>
</ul>



